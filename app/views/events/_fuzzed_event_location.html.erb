<% if user_signed_in? and (current_user === @event.host or current_user.attending_event?(@event)) %>
  <%= render :partial => 'locations/inline_with_paren_map', :locals => {:location => @event.venue.location} %>
<% else %>
  <%= @event.venue.location.neighborhood.try(:name) || "Unknown" %> Neighborhood
<% end %>