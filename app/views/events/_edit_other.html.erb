<div id="simple_index">
  <ul class="list-unstyled simple_index">
    <%= @other_events.each do |other| %>
       <li>
         <%= link_to edit_event_path(other) do %>
          <%= meeting_time(other.first_meeting, date_format: "%a %B", no_tz: true, show_end_time: false, show_livestamp: false) %>
         <% end %>
       </li>
    <% end %>
  </ul>
</div>